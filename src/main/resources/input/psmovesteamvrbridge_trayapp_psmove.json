{
   "bindings" : {
      "/actions/trayapp" : {
         "poses" : [
            {
               "output" : "/actions/trayapp/in/hand_left",
               "path" : "/user/hand/left/pose/raw"
            },
            {
               "output" : "/actions/trayapp/in/hand_right",
               "path" : "/user/hand/right/pose/raw"
            }
         ],
         "haptics" : [
            {
               "output" : "/actions/trayapp/out/haptic_right",
               "path" : "/user/hand/right/output/haptic"
            },
            {
               "output" : "/actions/trayapp/out/haptic_left",
               "path" : "/user/hand/left/output/haptic"
            }
         ],
         "sources" : [
            {
               "inputs" : {
                  "position" : {
                     "output" : "/actions/trayapp/in/analoginput"
                  }
               },
               "mode" : "trackpad",
               "path" : "/user/hand/right/input/trackpad"
            }
         ]
      }
   },
   "controller_type" : "vive_controller",
   "description" : "Bindings for the OpenVR SDK \"PSMoveSteamVRBridge TrayApp\" demo for the Vive controller",
   "name" : "PSMoveSteamVRBridge TrayApp bindings for Vive Controller"
}
